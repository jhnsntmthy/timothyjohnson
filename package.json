{
  "name": "anticoagulation_coagulation",
  "version": "1.0.0",
  "description": "### What is this repository for? ###",
  "main": "src/js/index.js",
  "config": {
    "buildpath": "dist"
  },
  "scripts": {
    "test": "tape-watch -r babel-register -1 test/*.test.js",
    "test:browser": "browserify test/browser/* -t [ babelify --presets [ es2015 ] ] | tap-closer | smokestack | tap-diff",
    "watch": "nodemon --watch src --watch img",
    "sync": "gulp browser-sync",
    "bundle": "gulp bundle",
    "presync": "npm run gulpsetup",
    "prebundle": "npm run gulpsetup",
    "gulpsetup": "npm-run-all clean-dist copy-files copy-webfonts-temp",
    "validate": "npm-run-all --parallel lint test build",
    "clean-dist": "rimraf $npm_package_config_buildpath",
    "copy-files": "cpy src/index.html src/manifest.json img/favicon/favicon.ico $npm_package_config_buildpath",
    "copy-images": "cp -R img $npm_package_config_buildpath",
    "copy-webfonts-temp": "cp -R fonts $npm_package_config_buildpath",
    "clean-and-copy": "npm-run-all clean-dist copy-files copy-webfonts-temp copy-images postcss",
    "lint": "eslint .",
    "postcss": "postcss -c postcss.config.json -o $npm_package_config_buildpath/app.css src/css/index.css"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "bootstrap-less": "^3.3.8",
    "jquery": "^1.12.4",
    "jquery-confirm": "3.0.1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.6",
    "browserify": "^13.3.0",
    "brunch": "^2.9.1",
    "cheet.js": "^0.3.3",
    "clean-css-brunch": "^2.0.0",
    "cpy": "^5.0.0",
    "cpy-cli": "^1.0.1",
    "css-brunch": "^2.6.1",
    "cssnano": "^3.10.0",
    "del": "^2.2.2",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.3.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "lodash": "^4.17.4",
    "markdown-brunch": "^1.7.2",
    "marked": "^0.3.6",
    "most": "^1.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "postcss": "^5.2.8",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-brunch": "^2.0.5",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "skatejs": "^4.5.2",
    "skatejs-web-components": "^5.0.1",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "tape-dom": "0.0.12",
    "tape-watch": "^2.2.4",
    "uglify-js-brunch": "^2.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^1.0.1",
    "watchify": "^3.8.0",
    "whatwg-fetch": "^2.0.1"
  }
}
